<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:scaleType="centerCrop"
        android:elevation="10dp"
        android:src="@drawable/gigi_hadid"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="568dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints">

            <TextView
                android:id="@+id/t1"
                android:layout_width="300dp"
                android:layout_height="100dp"
                android:layout_marginTop="320dp"
                android:background="@drawable/textview"
                android:elevation="@dimen/elevation"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.468"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/constraintLayout" />

            <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/shapeableImageView"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:elevation="@dimen/elevation"
        android:layout_marginTop="7dp"
        android:layout_marginStart="16dp"
        android:scaleType="centerCrop"
        android:src="@drawable/ggg"
        app:layout_constraintTop_toTopOf="@id/t1"
        app:layout_constraintStart_toStartOf="@id/t1"
        app:shapeAppearanceOverlay="@style/circle" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:elevation="@dimen/elevation"
        android:fontFamily="@font/abhaya_libre_medium"
        android:text="Gige Hadid"
        android:textColor="#EA8B6D"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/shapeableImageView"
        app:layout_constraintTop_toTopOf="@id/t1" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="17dp"
        android:layout_height="15dp"
        android:elevation="@dimen/elevation"
        android:src="@drawable/ic_baseline_location_on_24"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="@dimen/elevation"
        android:fontFamily="@font/abhaya_libre_medium"
        android:text="Los Angeles, California, U.S."
        android:textSize="11dp"
        android:textColor="#6E6E6E"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@id/textView"/>


    <TextView
        android:layout_width="66dp"
        android:layout_height="33dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/follow"
        android:elevation="@dimen/elevation"
        android:gravity="center"
        android:text="follow"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/t1" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="36dp"
        android:layout_height="19dp"
        android:layout_marginStart="12dp"
        android:elevation="@dimen/elevation"
        android:gravity="center"
        android:text="585"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:elevation="@dimen/elevation"
        android:gravity="center"
        android:text="followings"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@id/textView3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="48dp"
        android:elevation="@dimen/elevation"
        android:text="@string/likes"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView3" />

    <TextView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:elevation="@dimen/elevation"
        android:layout_marginStart="20dp"
        android:text="@string/photos"
        android:textSize="14sp"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <View
        android:id="@+id/view"
        android:layout_width="300dp"
        android:layout_height="1dp"
        android:layout_marginTop="56dp"
        android:background="#e0e0e0"
        android:elevation="@dimen/elevation"
        app:layout_constraintEnd_toEndOf="@id/t1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/t1"
        app:layout_constraintTop_toTopOf="@id/t1" />
    <TextView
        android:id="@+id/textView2"
        android:layout_width="58dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:elevation="@dimen/elevation"
        android:text="@string/followers"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/view" />

            <TextView
                android:id="@+id/t2"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginTop="120dp"
                android:fontFamily="@font/abhaya_libre_medium"
                android:paddingLeft="10dp"
                android:text="@string/text_view"
                android:textColor="@color/black"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/t1" />

            <View
                android:layout_width="405dp"
                android:layout_height="1dp"
                android:background="#e0e0e0"
                app:layout_constraintEnd_toEndOf="@id/t2"
                app:layout_constraintStart_toStartOf="@id/t2"
                app:layout_constraintTop_toBottomOf="@id/t2"
                tools:layout_editor_absoluteX="3dp"
                tools:layout_editor_absoluteY="413dp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="77dp"
                android:layout_height="38dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:gravity="center"
                android:text="Photos"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@id/textView6"
                app:layout_constraintTop_toTopOf="@id/textView6" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="411dp"
                android:layout_height="575dp"
                android:gravity="center"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/t2" />

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/shapeableImageView4"
                android:layout_width="180dp"
                android:layout_height="156dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="44dp"
                android:scaleType="centerCrop"
                android:src="@drawable/ggg"
                app:layout_constraintStart_toStartOf="@id/textView6"
                app:layout_constraintTop_toTopOf="@+id/textView6"
                app:shapeAppearanceOverlay="@style/square" />

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/shapeableImageView2"
                android:layout_width="175dp"
                android:layout_height="157dp"
                android:layout_marginStart="11dp"
                android:layout_marginTop="9dp"
                android:scaleType="centerCrop"
                android:src="@drawable/gg"
                app:layout_constraintStart_toEndOf="@+id/shapeableImageView4"
                app:layout_constraintTop_toBottomOf="@+id/shapeableImageView5"
                app:shapeAppearanceOverlay="@style/square" />

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/shapeableImageView3"
                android:layout_width="85dp"
                android:layout_height="90dp"
                android:layout_marginStart="8dp"
                android:scaleType="centerCrop"
                android:src="@drawable/g"
                app:layout_constraintStart_toEndOf="@id/shapeableImageView5"
                app:layout_constraintTop_toTopOf="@+id/shapeableImageView5"
                app:shapeAppearanceOverlay="@style/square" />

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/shapeableImageView5"
                android:layout_width="85dp"
                android:layout_height="90dp"
                android:layout_marginStart="10dp"
                android:scaleType="centerCrop"
                android:src="@drawable/gggg"
                app:layout_constraintStart_toEndOf="@id/shapeableImageView4"
                app:layout_constraintTop_toTopOf="@id/shapeableImageView4"
                app:shapeAppearanceOverlay="@style/square" />

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/shapeableImageView1"
                android:layout_width="85dp"
                android:layout_height="90dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="8dp"
                android:scaleType="centerCrop"
                android:src="@drawable/ggggg"
                app:layout_constraintStart_toEndOf="@id/shapeableImageView6"
                app:layout_constraintTop_toBottomOf="@+id/shapeableImageView4"
                app:shapeAppearanceOverlay="@style/square" />

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/shapeableImageView6"
                android:layout_width="85dp"
                android:layout_height="90dp"
                android:layout_marginTop="8dp"
                android:layout_marginRight="6dp"
                android:scaleType="centerCrop"
                android:src="@drawable/gggggg"
                app:layout_constraintStart_toStartOf="@id/shapeableImageView4"
                app:layout_constraintTop_toBottomOf="@id/shapeableImageView4"
                app:shapeAppearanceOverlay="@style/square" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="172dp"
                android:layout_marginTop="4dp"
                android:text="See more"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/shapeableImageView1" />

            <View
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="25dp"
                android:background="#e0e0e0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/shapeableImageView2" />

            <TextView
                android:id="@+id/textVideo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="40dp"
                android:text="Videos"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@id/shapeableImageView6"
                app:layout_constraintTop_toBottomOf="@+id/shapeableImageView6"
                tools:layout_editor_absoluteX="16dp" />

            <VideoView
                android:id="@+id/videoView"
                android:layout_width="match_parent"
                android:layout_height="177dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="48dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/view2"
                app:layout_constraintTop_toBottomOf="@id/view2" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>